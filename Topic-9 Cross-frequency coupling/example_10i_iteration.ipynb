   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "base = (1+2.3025/1024*1j)\n",
    "\n",
    "K=10000\n",
    "res = np.zeros([K,1], dtype=complex)\n",
    "for k in np.arange(K):\n",
    "    res[k] = base**k\n",
    "plt.plot(res.real)\n",
    "\n",
    "for k in np.arange(1001):\n",
    "    if np.mod(k,50)==0:\n",
    "        print(k, res[k])"
   ]
  }
